import java.util.Scanner;
public class maxarray {
	public static void main(String[] args) {
		int a[]=new int [10];
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int max=0;
		for(int i=0;i<n;i++) {
			a[i]=s.nextInt();
			if(max<a[i]) {
				max=a[i];
			}
		}
        System.out.println(max);
	}
}
